<?xml version="1.0" encoding="UTF-8"?>
<groups xmlns="http://www.isdc.ro/wro" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.isdc.ro/wro wro.xsd">
	<group name="statics">
		<css minimize="true">/assets/css/*.css</css>
		<js minimize="false">/assets/js/*.js</js>
		<group-ref>staticSass</group-ref>
	</group>
	<group name="staticSass">
		<css minimize="true">/assets/css/*.css</css>
		<group-ref>markups</group-ref>
	</group>
	<group name="markups">
		<css minimize="false">/**.css</css>
		<js minimize="false">/**.js</js>
	</group>
</groups>